    "commander": "^5.1.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "livebundle": "^0.1.11",
    "livebundle-sdk": "^0.1.9",
    "livebundle-utils": "^0.1.4",
    "micromatch": "^4.0.2",
    "shelljs": "^0.8.4"
  }
