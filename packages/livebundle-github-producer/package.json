{
  "name": "livebundle-github-producer",
  "version": "0.1.6",
  "main": "dist/index.js",
  "bin": {
    "livebundle-github-producer": "dist/cli.js"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo",
    "compile": "tsc -b tsconfig.build.json",
    "docker:build": "yarn build && docker build -t livebundle/github-producer .",
    "prepublishOnly": "yarn build",
    "start": "node dist/cli.js",
    "test": "mocha"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "amqp-connection-manager": "^3.2.0",
    "amqplib": "^0.6.0",
    "commander": "^6.0.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "livebundle-utils": "^0.1.6"
  }
}
