{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://electrode.io/schemas/github-producer-config.json",
  "type": "object",
  "properties": {
    "server": {
      "type": "object",
      "properties": {
        "host": {
          "type": "string",
          "format": "hostname"
        },
        "port": {
          "type": "number",
          "minimum": 0,
          "maximum": 65535
        }
      }
    },
    "queue": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        }
      }
    }
  }
}
